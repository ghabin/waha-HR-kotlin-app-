<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragSearch"
    >



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="714dp"
        android:layout_margin="10dp"
        android:orientation="horizontal"
        android:scrollbarAlwaysDrawVerticalTrack="true">

        <LinearLayout
            android:layout_width="132dp"
            android:layout_height="match_parent"
            android:layout_gravity="fill"
            android:orientation="vertical"
            android:scrollbars="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="74dp"
                android:orientation="horizontal">

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fab"
                    style="@style/Widget.MaterialComponents.BottomAppBar.Colored"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|top"
                    android:layout_marginTop="5dp"
                    android:clickable="true"
                    android:src="@color/yellow" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fabClear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="5dp"
                    android:clickable="true"
                    android:src="@android:drawable/btn_dialog" />

            </LinearLayout>


            <TextView
                android:id="@+id/tvAname"
                android:layout_width="@dimen/tv_width"
                android:layout_height="60dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:gravity="center_vertical|center"
                android:text="Aname"
                android:textSize="@dimen/tv_textSize" />

            <TextView
                android:id="@+id/tvename"
                android:layout_width="@dimen/tv_width"
                android:layout_height="60dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:gravity="center_vertical|center"
                android:text="Ename"
                android:textSize="@dimen/tv_textSize" />


            <TextView
                android:id="@+id/bDate"
                android:layout_width="@dimen/tv_width"
                android:layout_height="60dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:gravity="center_vertical|center"
                android:text="bDate"
                android:textSize="@dimen/tv_textSize" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="@dimen/tv_width"
                android:layout_height="60dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:gravity="center_vertical|center"
                android:text="dept"
                android:textSize="@dimen/tv_textSize" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="@dimen/tv_width"
                android:layout_height="60dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:gravity="center_vertical|center"
                android:text="grade"
                android:textSize="@dimen/tv_textSize" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="@dimen/tv_width"
                android:layout_height="60dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:gravity="center_vertical|center"
                android:text="location"
                android:textSize="@dimen/tv_textSize" />

            <!--            <Button-->
<!--                android:id="@+id/btnSave"-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_height="38dp"-->
<!--                android:text="btnSaveData" />-->


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingLeft="10dp"
            android:orientation="vertical">


            <EditText
                android:id="@+id/ed_search"
                android:layout_width="match_parent"
                android:layout_height="@dimen/editText_height"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:ems="10"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textSize="@dimen/tv2_textSize"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/ed_Aname"
                android:layout_width="match_parent"
                android:layout_height="@dimen/editText_height"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:ems="10"
                android:text="Aname"
                android:textAlignment="center"
                android:textSize="@dimen/tv2_textSize"
                />


            <TextView
                android:id="@+id/ed_Ename"
                android:layout_width="match_parent"
                android:layout_height="@dimen/editText_height"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:ems="10"
                android:text="ed_Ename"
                android:textAlignment="center"
                android:textSize="18sp"
                />


            <TextView
                android:id="@+id/ed_bdate"
                android:layout_width="match_parent"
                android:layout_height="@dimen/editText_height"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="ed_bdate"
                android:textAlignment="center"
                android:textSize="@dimen/tv2_textSize"
                />


            <TextView
                android:id="@+id/ed_Dept"
                android:layout_width="match_parent"
                android:layout_height="@dimen/editText_height"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="ed_Dept"
                android:textAlignment="center"
                android:textSize="@dimen/tv2_textSize"
                />


            <TextView
                android:id="@+id/ed_Grade"
                android:layout_width="match_parent"
                android:layout_height="@dimen/editText_height"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="ed_Grade"
                android:textAlignment="center"
                android:textSize="@dimen/tv2_textSize"
                />


            <TextView
                android:id="@+id/ed_assignCat"
                android:layout_width="match_parent"
                android:layout_height="@dimen/editText_height"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/layout_marginBottom"
                android:background="@drawable/background_selector"
                android:ems="10"
                android:text="ed_assignCat"
                android:textAlignment="center"
                android:textSize="@dimen/tv2_textSize"
                />

            <Button
                android:id="@+id/btnSave2"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:visibility="invisible"
                android:text="Save2" />

        </LinearLayout>

    </LinearLayout>

    </FrameLayout>